{
  "name": "nearley",
  "displayName": "Nearley",
  "description": "Nearley support for Visual Studio Code",
  "icon": "logo.png",
  "version": "1.1.0",
  "publisher": "hikerpig",
  "engines": {
    "vscode": "^0.10.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hikerpig/nearley-vscode"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:nearley"
  ],
  "main": "./out/src/extension",
  "license": "MIT",
  "contributes": {
    "languages": [
      {
        "id": "nearley",
        "extensions": [
          ".ne"
        ],
        "aliases": [
          "Nearley",
          "nearley"
        ],
        "filenames": [
          ".ne"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nearley",
        "scopeName": "source.nearley",
        "path": "./syntaxes/nearley.tmLanguage.json",
        "embeddedLanguages": {
          "source.ts": "typescript"
        }
      }
    ]
  },
  "devDependencies": {
    "vscode": "0.10.x"
  }
}
